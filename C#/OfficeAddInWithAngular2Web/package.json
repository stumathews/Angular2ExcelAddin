    "@angular/core": "5.0.5",
    "@angular/forms": "5.0.5",
    "@angular/http": "5.0.5",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "5.0.5",
    "@angular/platform-browser-dynamic": "5.0.5",
    "@angular/router": "5.0.5",
    "@angular/upgrade": "5.0.5",
    "core-js": "2.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.3",
    "systemjs": "0.20.19",
    "zone.js": "0.8.18"
